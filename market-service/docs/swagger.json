            "properties": {
                "buy_price": {
                    "type": "number"
                },
                "buy_quantity": {
                    "type": "integer"
                },
                "figi": {
                    "type": "string"
                },
                "sell_price": {
                    "type": "number"
                },
                "sell_quantity": {
                    "type": "integer"
                }
            }
