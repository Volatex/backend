                "TariffPremium": "0% commission",
                "TariffTrader": "0.04% commission"
            },
            "x-enum-varnames": [
                "TariffInvestor",
                "TariffTrader",
                "TariffPremium"
            ]
        },
        "request.SaveStrategy": {
            "type": "object",
            "required": [
                "buy_price",
                "buy_quantity",
                "figi",
                "sell_price",
                "sell_quantity"
