            "properties": {
                "buyPrice": {
                    "type": "number"
                },
                "buyQuantity": {
                    "type": "integer"
                },
                "createdAt": {
                    "type": "string"
                },
                "figi": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "sellPrice": {
                    "type": "number"
                },
                "sellQuantity": {
                    "type": "integer"
                },
